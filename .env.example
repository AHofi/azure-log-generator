# Application Insights Configuration
APPLICATIONINSIGHTS_CONNECTION_STRING=InstrumentationKey=your-key-here;IngestionEndpoint=https://region.in.applicationinsights.azure.com/;LiveEndpoint=https://region.livediagnostics.monitor.azure.com/
APP_INSIGHTS_DAILY_CAP_GB=1
AI_SAMPLING_PERCENTAGE=100

# Application Configuration
PORT=8080
NODE_ENV=production
LOG_GENERATION_MAX_THREADS=20
DEFAULT_LOG_SIZE_BYTES=1024

# Feature Flags
ENABLE_CUSTOM_METRICS=true
ENABLE_CUSTOM_EVENTS=true
ENABLE_DEPENDENCIES_TRACKING=true

# Azure App Service (automatically set when deployed)
# WEBSITE_INSTANCE_ID=
# WEBSITE_SITE_NAME=